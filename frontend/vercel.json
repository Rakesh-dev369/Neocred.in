{
  "framework": "vite",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/.well-known/(.*)",
      "dest": "/.well-known/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets/|robots.txt|sitemap.xml|.well-known/).*)",
      "destination": "/index.html"
    }
  ]
}