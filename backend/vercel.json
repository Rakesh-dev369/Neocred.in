{
  "version": 2,
  "builds": [
    {
      "src": "main_simple.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main_simple.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key"
  },
  "functions": {
    "main_simple.py": {
      "maxDuration": 30
    }
  }
}