# Real-time & Streaming
WEBSOCKET_ENABLED=true
SSE_ENABLED=true
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
RABBITMQ_URL=amqp://localhost:5672

# AutoML & LLMs
