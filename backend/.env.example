# LLM-Powered AutoML Pipeline
MLFLOW_TRACKING_URI=sqlite:///mlflow.db
MLFLOW_EXPERIMENT_NAME=neocred_credit_risk
MLFLOW_MODEL_REGISTRY=neocred_credit_model
